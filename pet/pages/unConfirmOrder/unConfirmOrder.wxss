}

.bill-info-pay-money {
  text-align: right;
  width: 50%;
  font-size: 32rpx;
  font-family: sans-serif;
  font-weight: bold;
  color: #F1B000;
}

.bill-info-pay-label {
  font-family: sans-serif;
  font-size: 32rpx;
  width: 20%;
}


.bill-info-pet-type {
  text-align: left;
  width: 33%;
  font-size: 32rpx;
  margin-left: 20rpx;
  font-family: sans-serif;
}

.bill-info-pet-info {
  text-align: center;
  width: 20%;
  font-size: 32rpx;
  font-family: sans-serif;
}

.bill-info-pet-money {
  color: #F1B000;
}

.bill-info-pet-mode {
  text-align: center;
  width: 30%;
