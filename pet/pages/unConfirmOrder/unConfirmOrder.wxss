}

.bill-image-area{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  width: 100%;
}

.bill-image-box{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 25%;
  height: 200rpx;
  position: relative;
}

.bill-image{
  width: 95%;
  height: 95%;
}

.bill-image-delete{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  background: white;
  border-radius: 25rpx;
  z-index: 1000;
}

.bill-video-area{
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.bill-video-box{
  position: relative;
  margin: 10rpx auto;
}

.video{

}

.empty-area{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 160rpx 40rpx;
  background: #00000000;
}

.empty-content{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.empty-title{
