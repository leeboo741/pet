  margin: 10rpx ;
}

.bill-info-remark-content-box{
  display: flex;
  flex-direction: column;
  flex-grow: 1;
}

.bill-info-remark-item{
  display: flex;
  flex-direction: column;
  margin: 10rpx ;
}

.bill-info-remark-item-content{
  font-size: 32rpx;
  flex-wrap: wrap;
  flex-grow: 1;
  word-wrap: break-word;
  word-break: break-all;
}

.bill-info-remark-item-time{
