  border-radius: 20px;
  justify-content: left;
  flex-wrap: wrap;
  background-color: white;
  align-items: center;
}

.bill-info-icon {
  width: 20rpx;
  margin-left: 20rpx;
  height: 20rpx;
  border: solid #FF8C00 1rpx;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #FF8C00;
}

.bill-info-icon icon {
  font-size: 20rpx;
  line-height: 20;
  color: red;
}

.bill-info {
  display: flex;
  justify-content: left;
  align-items: center;
}

.bill-info-billNo {
  margin-left: 10rpx;
  flex-grow: 1;
  font-size: 32rpx;
  text-align: left;
}

.bill-info-content {
  flex-direction: column;
  display: flex;
  justify-content: left;
  align-items: center;
}

.bill-info-way {
  display: flex;
  width: 100%;
  margin-bottom: 20rpx;
  justify-content: left;
  align-items: center;
}

.bill-info-way-date {
  flex-grow: 1;
  font-size: 32rpx;
  text-align: left;
  margin-left: 20rpx;
  font-family: sans-serif;
}

.bill-info-way-start {
  flex-grow: 1;
  text-align: right;
  font-size: 32rpx;
  font-family: sans-serif;
}

.bill-info-way-split {
  flex-grow: 1;
  text-align: center;
