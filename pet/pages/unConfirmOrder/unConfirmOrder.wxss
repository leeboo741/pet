}

.in-out-port-info-input{
  border-radius: 10rpx;
  border: 1rpx solid lightgray;
  font-size: 32rpx;
  flex-grow: 1;
  padding: 10rpx;
}

.text-normal{
  color: black;
}

.text-placeholder{
  color: lightgray;
}

.in-out-port-info-button{
  padding: 8rpx 25rpx;
  margin-left: 15rpx;
  flex-shrink: 0;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 32rpx;
  background: #2d8cf0;
  color: white;
  border-radius: 8rpx
}

.bill-operate-box{
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: flex-end;
}

.bill-operate-button{
  margin: 8rpx;
  font-size: 30rpx;
  color: white;
  background: rgb(100, 149, 237);
