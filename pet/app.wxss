/**app.wxss**/

.page-white{
  background: white;
}

.container {
  width: 760rpx;
  height: 100%;
} 

.container_page{
  height: 100%;
  display: flex;
  flex-direction: column;
  background: rgb(220, 220, 220);
  /* min-height: 100%; */
  max-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.page-box {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgb(220, 220, 220);
  display: flex;
  flex-direction: column;
}

.page-content-scroll-full {
  width: 100%;
  height: 100%;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.input-normal{
  background: rgb(230, 230, 230);
  padding: 10rpx 20rpx;
  margin: 30rpx 30rpx 30rpx 30rpx;
  vertical-align: middle;
  font-size: 30rpx;
  font-family: "Microsoft YaHei";
  border-color: lightblue;
  border-width: 1rpx;
  border-radius: 20rpx;
  height: 60rpx;
}

.input-under {
  font-size: 18px;
  height: 30px;
  font-family: "Microsoft YaHei";
  background: white;
  border-bottom:1px ridge #C7C7C7;
}

.input-normal-placeholder{
  font-size: 18px;
  color: #D4D4D4;
  font-family: sans-serif;
}


.input-right {
    text-align: right;
}

#button{
  border-radius: 15rpx;
}

.button-blue{
  background-color: rgb(100, 149, 237);
  color: white;
}

.block-fillet {
  background-color: #ffffff;
  width: 730rpx;
  margin: 5px 5px 5px 5px;  
  border-radius: 5px;
  box-shadow: 0px -2px 5px 2px #F4F4F4;
}

.popview{
  position: fixed;
  width: 100%;
  height: 100%;
}

.popview-mask{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(14, 13, 13, 0.425);
}


.popview-content{
  position: relative;
  background: white;
  width: 90%;
  margin: 25% auto;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  overflow:hidden;
}

.popview-content-title{
  background: rgb(100, 149, 237);
  padding: 20rpx 20rpx;
  font-family: "Microsoft YaHei";
  font-style: blod;
  font-size: 45rpx; 
  color: white;
}

.popview-content-content{
  height: 500rpx;
}

.popview-content-button-box{
  display: flex;
  flex-direction: row;
  margin: 20rpx 30rpx;
}

.popview-content-button{
  font-size: 30rpx;
  font-family: "Microsoft YaHei";
  font-style: blod;
  flex-grow: 1;
}

.justify-between{
  justify-content: space-between;
}

.row{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 10rpx 20rpx;
}

